        <Label Content="Date de naissance :" HorizontalAlignment="Left" Margin="25,108,0,0" VerticalAlignment="Top" Width="130"/>
        <TextBox x:Name="txtNss" HorizontalAlignment="Left" Height="30" Margin="573,32,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="140" Padding="0,4,0,0" TabIndex="5"/>
        <Label Content="Numéro NAS :" HorizontalAlignment="Left" Margin="467,32,0,0" VerticalAlignment="Top" Width="100"/>
        <DatePicker x:Name="dateNaissance" HorizontalAlignment="Left" Margin="160,107,0,0" VerticalAlignment="Top" Height="30" Padding="2,4,2,2" TabIndex="3"/>
        <TextBox x:Name="txtAdresse" HorizontalAlignment="Left" Height="30" Margin="99,231,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="398" Padding="0,4,0,0" TabIndex="8"/>
        <Label Content="Adresse :" HorizontalAlignment="Left" Margin="25,231,0,0" VerticalAlignment="Top" Width="68"/>
        <TextBox x:Name="txtVille" HorizontalAlignment="Left" Height="30" Margin="102,270,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="147" Padding="0,4,0,0" TabIndex="9"/>
        <Label Content="Ville :" HorizontalAlignment="Left" Margin="25,270,0,0" VerticalAlignment="Top" Width="68"/>
        <TextBox x:Name="txtProvince" HorizontalAlignment="Left" Height="30" Margin="558,269,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="155" Padding="0,4,0,0" TabIndex="11"/>
        <Label Content="Province :" HorizontalAlignment="Left" Margin="476,269,0,0" VerticalAlignment="Top" Width="77"/>
        <TextBox x:Name="txtCodePostal" HorizontalAlignment="Left" Height="30" Margin="364,270,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="96" Padding="0,4,0,0" TabIndex="10"/>
        <Label Content="Code postal :" HorizontalAlignment="Left" Margin="265,269,0,0" VerticalAlignment="Top" Width="92"/>
        <TextBox x:Name="txtTelephone" HorizontalAlignment="Left" Height="30" Margin="160,144,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="169" Padding="0,4,0,0" TabIndex="4"/>
        <Label Content="Téléphone :" HorizontalAlignment="Left" Margin="25,145,0,0" VerticalAlignment="Top" Width="87"/>
        <TextBox x:Name="txtNumAssurance" HorizontalAlignment="Left" Height="30" Margin="574,108,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="139" Padding="0,4,0,0" TabIndex="6"/>
        <Label Content="N° assurance :" HorizontalAlignment="Left" Margin="467,108,0,0" VerticalAlignment="Top" Width="100"/>
        <TextBox x:Name="txtCompagnie" HorizontalAlignment="Left" Height="30" Margin="574,145,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="141" Padding="0,4,0,0" TabIndex="7"/>
        <Label Content="Compagnie :" HorizontalAlignment="Left" Margin="467,144,0,0" VerticalAlignment="Top" Width="100"/>
    </Grid>
</Window>
