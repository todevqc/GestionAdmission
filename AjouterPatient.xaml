<Window x:Class="Projet_Integrateur_ToufikDahmani.AjouterPatient"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projet_Integrateur_ToufikDahmani"
        mc:Ignorable="d"
        Title="Ajouter un Patient" Height="450" Width="755.878" WindowStartupLocation="CenterScreen" FontSize="14" ResizeMode="NoResize">
    <Grid>
        <Button x:Name="btnEnregistrerPatient" Content="Enregistrer" HorizontalAlignment="Left" Margin="139,362,0,0" VerticalAlignment="Top" Width="126" Height="34" FontSize="16" Click="BtnEnregistrerPatient_Click" TabIndex="12"/>
        <Button x:Name="btnViderChamps" Content="Reset" HorizontalAlignment="Left" Margin="303,362,0,0" VerticalAlignment="Top" Width="126" Height="34" FontSize="16" Click="BtnViderChamps_Click" TabIndex="13"/>
        <Button x:Name="btnAnnuler" Content="Annuler" HorizontalAlignment="Left" Margin="468,362,0,0" VerticalAlignment="Top" Width="126" Height="34" FontSize="16" Click="BtnAnnuler_Click" TabIndex="14"/>
        <TextBox x:Name="txtNom" HorizontalAlignment="Left" Height="30" Margin="160,32,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="255" Padding="0,4,0,0" TabIndex="1"/>
        <Label Content="Nom :" HorizontalAlignment="Left" Margin="25,32,0,0" VerticalAlignment="Top" Width="130"/>
        <TextBox x:Name="txtPrenom" HorizontalAlignment="Left" Height="30" Margin="160,70,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="255" Padding="0,4,0,0" TabIndex="2"/>
